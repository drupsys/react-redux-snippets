{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "time", "timeEnd", "trace"]
    },
    "object-literal-sort-keys": false,
    "no-namespace": false,
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "semicolon": [true, "always"],
    "no-empty": false,
    "quotemark": [true, "double"]
  }
}
